{
"name":"QQR",
"description":"快速生成二维码.",
"version":"1.0.0",
"manifest_version":2,
"icons": {
      "48": "1.png"
   },
"browser_action": {
  "default_icon": "1.png",
  "default_title": "Quick QR Code"
},
"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/jq3.4.1.min.js", "./js/qrcode.js", "./js/content-script.js"],
      "run_at": "document_start"
    }
],
"web_accessible_resources": ["js/jq3.4.1.min.js", "js/inject.js"],
"background":
{
	"scripts": ["./js/jq3.4.1.min.js", "./js/qrcode.js", "./js/background.js"]
},
"permissions":[
		"contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"http://*/*",
		"https://*/*"
],
	"homepage_url": "https://zxx.im"
}
